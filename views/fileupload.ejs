<div class="container text-white">
  <h1>File Upload</h1>

  <form
    action="/authorprofile/<%= author %> "
    method="POST"
    enctype="multipart/form-data"
  >
    <div>
      <div class="form-group">
        <span>File</span>
        <input name="myImage" type="file" />
      </div>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
  <br />
</div>
